{
  "schemaVersion": 1,
  "id": "barrymore_minecraft_binding",
  "version": "${version}",

  "name": "Barrymore Minecraft Binding",
  "description": "Minecraft binding for Barrymore smart house project",
  "authors": [
    "UQuark"
  ],

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.uquark.barrymoreminecraftbinding.BarrymoreMinecraftBinding"
    ],
    "client": [
      "me.uquark.barrymoreminecraftbinding.BarrymoreMinecraftBindingClient"
    ]
  },
  "mixins": [
    "barrymore_minecraft_binding.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.15.x",
    "com_google_cloud_google-cloud-speech": "1.23.0"
  },
  "suggests": {
    "flamingo": "*"
  }
}
